    transform: scale(0.8);
    transition: all 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

.scale-in.visible {
    opacity: 1;
    transform: scale(1);
}

/* Smooth scrolling for anchor links */
.smooth-scroll {
    scroll-behavior: smooth;
}

/* Keyframe Animations */
@keyframes float {
    0%, 100% { transform: translateY(0px); }
    50% { transform: translateY(-20px); }
}

@keyframes central-pulse {
    0%, 100% { 
        transform: translate(-50%, -50%) scale(1);
        box-shadow: 0 0 30px rgba(255, 255, 255, 0.3);
    }
    50% { 
        transform: translate(-50%, -50%) scale(1.1);
        box-shadow: 0 0 50px rgba(255, 255, 255, 0.5);
    }
}

@keyframes central-rotate {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

@keyframes orbit-rotate {
    0% { transform: translate(-50%, -50%) rotate(0deg); }
    100% { transform: translate(-50%, -50%) rotate(360deg); }
}

@keyframes counter-rotate {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(-360deg); }
}

@keyframes glow-pulse {
    0%, 100% { 
        opacity: 0.3;
        transform: scale(1);
    }
    50% { 
        opacity: 0.6;
        transform: scale(1.2);
    }
}

@keyframes pulse-subtle {
    0%, 100% { transform: scale(1); }
    50% { transform: scale(1.05); }
}

@keyframes pulse-strong {
    0% { transform: scale(1); }
    50% { transform: scale(1.2) rotate(10deg); }
    100% { transform: scale(1.15) rotate(10deg); }
}

@keyframes pulse-glow {
    0%, 100% { 
        transform: scale(1);
        filter: drop-shadow(0 0 5px currentColor);
    }
    50% { 
        transform: scale(1.1);
        filter: drop-shadow(0 0 15px currentColor);
    }
}

@keyframes pulse-cta {
    0%, 100% { 
        transform: scale(1);
        box-shadow: 0 4px 15px rgba(255, 193, 7, 0.4);
    }
    50% { 
        transform: scale(1.05);
        box-shadow: 0 8px 25px rgba(255, 193, 7, 0.6);
    }
}

@keyframes bounce-arrow {
    0%, 100% { 
        transform: translateY(0);
        opacity: 0.8;
    }
    50% { 
        transform: translateY(5px);
        opacity: 1;
    }
}

@keyframes scroll-line {
    0%, 100% { 
        opacity: 0.6;
        transform: scaleY(1);
    }
    50% { 
        opacity: 1;
        transform: scaleY(1.2);
    }
}

@keyframes scroll-indicator {
    0% { opacity: 0; top: 10px; }
    50% { opacity: 1; top: 20px; }
    100% { opacity: 0; top: 30px; }
}

@keyframes rotate-slow {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

/* Color Scheme Override - Medical Blue/Green/Orange Theme */
/* Override all yellow/amber buttons with new color scheme */

/* Primary Buttons - Blue Theme */
.cta-primary,
.hero-btn-primary,
.btn-cta-primary,
button.btn-primary,
a.btn-primary,
.cta-buttons .cta-primary {
    background: linear-gradient(135deg, #3b82f6 0%, #1d4ed8 100%) !important;
    border-color: #3b82f6 !important;
    color: white !important;
    box-shadow: 0 4px 15px rgba(59, 130, 246, 0.4) !important;
}

.cta-primary:hover,
.cta-primary:focus,
.hero-btn-primary:hover,
.hero-btn-primary:focus,
.btn-cta-primary:hover,
.btn-cta-primary:focus,
button.btn-primary:hover,
button.btn-primary:focus,
a.btn-primary:hover,
a.btn-primary:focus,
.cta-buttons .cta-primary:hover,
.cta-buttons .cta-primary:focus {
    background: linear-gradient(135deg, #2563eb 0%, #1e40af 100%) !important;
    border-color: #2563eb !important;
    color: white !important;
    box-shadow: 0 6px 20px rgba(59, 130, 246, 0.6) !important;
    text-decoration: none !important;
}

/* Secondary Buttons - Orange Theme */
.cta-secondary,
.hero-btn-secondary,
.btn-cta-secondary,
button.btn-secondary,
a.btn-secondary,
.cta-buttons .cta-secondary {
    background: linear-gradient(135deg, #f97316 0%, #ea580c 100%) !important;
    border-color: #f97316 !important;
    color: white !important;
    box-shadow: 0 4px 15px rgba(249, 115, 22, 0.4) !important;
}

.cta-secondary:hover,
.cta-secondary:focus,
.hero-btn-secondary:hover,
.hero-btn-secondary:focus,
.btn-cta-secondary:hover,
.btn-cta-secondary:focus,
button.btn-secondary:hover,
button.btn-secondary:focus,
a.btn-secondary:hover,
a.btn-secondary:focus,
.cta-buttons .cta-secondary:hover,
.cta-buttons .cta-secondary:focus {
    background: linear-gradient(135deg, #ea580c 0%, #dc2626 100%) !important;
    border-color: #ea580c !important;
    color: white !important;
    box-shadow: 0 6px 20px rgba(249, 115, 22, 0.6) !important;
    text-decoration: none !important;
}

/* Success Buttons - Green Theme */
.btn-success,
button.btn-success,
a.btn-success {
    background: linear-gradient(135deg, #10b981 0%, #059669 100%) !important;
    border-color: #10b981 !important;
    color: white !important;
    box-shadow: 0 4px 15px rgba(16, 185, 129, 0.4) !important;
}

.btn-success:hover,
.btn-success:focus,
button.btn-success:hover,
button.btn-success:focus,
a.btn-success:hover,
a.btn-success:focus {
    background: linear-gradient(135deg, #059669 0%, #047857 100%) !important;
    border-color: #059669 !important;
    color: white !important;
    box-shadow: 0 6px 20px rgba(16, 185, 129, 0.6) !important;
    text-decoration: none !important;
}

/* Stats Section Background */
.stats-section {
    background: linear-gradient(135deg, #f8fafc 0%, #e2e8f0 100%) !important;
}

.stat-card {
    background: white !important;
    border: 1px solid #e2e8f0 !important;
}

.stat-card:hover {
    border-color: #3b82f6 !important;
    box-shadow: 0 10px 25px rgba(59, 130, 246, 0.15) !important;
}

/* Features Section */
.features-section {
    background: white !important;
}

.feature-card {
    background: white !important;
    border: 1px solid #e2e8f0 !important;
}

.feature-card:hover {
    border-color: #3b82f6 !important;
    box-shadow: 0 15px 35px rgba(59, 130, 246, 0.15) !important;
}

/* Subject Cards */
.subject-card {
    background: white !important;
    border: 1px solid #e2e8f0 !important;
}

.subject-card:hover {
    border-color: #3b82f6 !important;
    box-shadow: 0 15px 35px rgba(59, 130, 246, 0.15) !important;
}

.subject-card::before {
    background: linear-gradient(90deg, #3b82f6, #10b981) !important;
}

.subject-card:hover .subject-icon i {
    color: #3b82f6 !important;
}

.subject-btn {
    background: var(--btn-primary-bg) !important;
    border-color: #3b82f6 !important;
    color: white !important;
}

.subject-btn:hover {
    background: var(--btn-primary-hover) !important;
    box-shadow: 0 4px 15px rgba(59, 130, 246, 0.3) !important;
}

/* Override any remaining yellow/amber colors */
*[style*="background-color: #ffc107"],
*[style*="background: #ffc107"],
*[class*="bg-warning"],
*[class*="text-warning"] {
    background: var(--btn-secondary-bg) !important;
    color: white !important;
}

/* Text Colors */
.text-primary {
    color: #374151 !important;
}

.text-secondary {
    color: #6b7280 !important;
}

.text-blue {
    color: #3b82f6 !important;
}

.text-green {
    color: #10b981 !important;
}

.text-orange {
    color: #f97316 !important;
}

.text-navy {
    color: #1e3a8a !important;
}

/* Icon Colors */
.text-primary i,
.text-blue i {
    color: #3b82f6 !important;
}

.text-success i,
.text-green i {
    color: #10b981 !important;
}

.text-warning i,
.text-orange i {
    color: #f97316 !important;
}

/* Responsive Button Adjustments */
@media (max-width: 768px) {
    .cta-primary,
    .cta-secondary,
    .hero-btn-primary,
    .hero-btn-secondary {
        width: 100%;
        margin-bottom: 1rem;
        justify-content: center;
    }
}
